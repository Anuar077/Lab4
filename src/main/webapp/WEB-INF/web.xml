<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
   <!--Session-->
    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>Session.FirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/servlet1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>Session.SecondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/servlet2</url-pattern>
    </servlet-mapping>

    <!--Login and Logout-->
    <servlet>
        <description></description>
        <display-name>Login</display-name>
        <servlet-name>Login</servlet-name>
        <servlet-class>LoginandLogout.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>Profile</display-name>
        <servlet-name>Profile</servlet-name>
        <servlet-class>LoginandLogout.Profile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/Profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>Logout</display-name>
        <servlet-name>Logout</servlet-name>
        <servlet-class>LoginandLogout.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>


    <!--HiddenFromField-->
    <servlet>
        <servlet-name>s3</servlet-name>
        <servlet-class>HiddenFromField.FirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s3</servlet-name>
        <url-pattern>/servlet3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>HiddenFromField.SecondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/servlet4</url-pattern>
    </servlet-mapping>

    <!--URL-->
    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>URL.FirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/servlet5</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>s6</servlet-name>
        <servlet-class>URL.SecondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s6</servlet-name>
        <url-pattern>/servlet6</url-pattern>
    </servlet-mapping>

    <!--HTTP-->
    <servlet>
        <servlet-name>s7</servlet-name>
        <servlet-class>HTTP.FirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s7</servlet-name>
        <url-pattern>/servlet7</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>s8</servlet-name>
        <servlet-class>HTTP.SecondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>s8</servlet-name>
        <url-pattern>/servlet8</url-pattern>
    </servlet-mapping>


    <!--LoginWithoutCookie-->
    <servlet>
        <description></description>
        <display-name>LoginServlet</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginWithoutCookie.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>ProfileServlet</display-name>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>LoginWithoutCookie.ProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/ProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>LogoutServlet</display-name>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LoginWithoutCookie.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
</web-app>